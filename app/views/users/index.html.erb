<h1>All Users</h1>
  <ul>

    <% @users.each do |user| %>
      <li>
      Email: <%= user.email %>
      <%= link_to 'settings', edit_user_path(user) %>
      <%= link_to 'delete', user, method: :delete,
                              data: {confirm: "You sure?" } %>
      </li>
    <% end %>

  </ul>
